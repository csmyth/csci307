<%= simple_form_for(@observation) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :def_fee %>
    <%= f.input :title %>
    <%= f.input :archived %>
    <%= f.input :archive_date %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
